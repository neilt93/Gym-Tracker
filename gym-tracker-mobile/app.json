{
  "expo": {
    "name": "Gym Tracker",
    "slug": "gym-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.gymtracker",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app uses HealthKit to track your workouts and fitness data.",
        "NSHealthUpdateUsageDescription": "This app saves your workout data to HealthKit."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      },
      "package": "com.yourcompany.gymtracker"
    },
    "web": {
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "gym-tracker"
  }
} 